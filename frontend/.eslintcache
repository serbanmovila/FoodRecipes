s = this.props.recipes\r\n        return (\r\n            <>\r\n                <Container>\r\n                    <Header>\r\n                        <HeaderContent>\r\n                            <h3>Recipes</h3>\r\n                            <p>\r\n                                View, add, edit and remove recipes of all kinds.\r\n                            </p>\r\n                        </HeaderContent>\r\n                        <Button endIcon={<AddCircleIcon />} onClick={this.open}>\r\n                            New Recipe\r\n                        </Button>\r\n                    </Header>\r\n                    <RecipesListing>\r\n                        {recipes.map((recipe) => {\r\n                            return (\r\n                                <Recipe\r\n                                    handleClick={() => {\r\n                                        this.viewRecipe(recipe)\r\n                                    }}\r\n                                    data={recipe}\r\n                                />\r\n                            )\r\n                        })}\r\n                    </RecipesListing>\r\n                </Container>\r\n                <Modal close={this.close} open={this.state.openModal}>\r\n                    <AddRecipeForm\r\n                        close={this.close}\r\n                        preloaded={this.state.preloaded}\r\n                        data={this.state.recipeData}\r\n                    />\r\n                </Modal>\r\n            </>\r\n        )\r\n    }\r\n}\r\n\r\nexport default connect(\r\n    (state) => ({\r\n        recipes: state.recipes\r\n    }),\r\n    { getRecipes }\r\n)(RecipesScreen)\r\n","C:\\Users\\Serban\\Desktop\\licentageorgiana\\FoodRecipes\\frontend\\src\\Helpers\\Header\\Header.js",["128","129","130","131","132"],"import React from 'react'\r\nimport styled from 'styled-components'\r\nimport { Link } from 'react-router-dom'\r\nimport { constants as c } from '../Constants'\r\