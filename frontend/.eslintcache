  close = () => {\r\n        this.setState({\r\n            openModal: false,\r\n            preloaded: false\r\n        })\r\n    }\r\n\r\n    open = () => {\r\n        this.setState({\r\n            openModal: true\r\n        })\r\n    }\r\n\r\n    openIngredient = (data) => {\r\n        this.setState(\r\n            {\r\n                preloaded: true,\r\n                data: data\r\n            },\r\n            () => {\r\n                this.setState({\r\n                    openModal: true\r\n                })\r\n            }\r\n        )\r\n    }\r\n\r\n    deleteIngredient = (id) => {\r\n        this.props.deleteIngredient(id, this.props.getIngredients)\r\n    }\r\n\r\n    componentDidMount() {\r\n        this.props.getIngredients()\r\n    }\r\n\r\n    render() {\r\n        const { ingredients } = this.props\r\n        return (\r\n            <>\r\n                <Container>\r\n                    <Header>\r\n                        <HeaderContent>\r\n                            <h3>Ingredients</h3>\r\n                            <p>\r\n                                View, add, edit and remove ingredients of all\r\n                                kinds.\r\n                            </p>\r\n                        </HeaderContent>\r\n                        <Button endIcon={<AddCircleIcon />} onClick={this.open}>\r\n                            New Ingredient\r\n                        </Button>\r\n                    </Header>\r\n                    <TableContainer>\r\n                        <TableHeader />\r\n                        <IngredientsList>\r\n                            {ingredients.map((ingredient) => {\r\n                                return (\r\n                                    <Ingredient\r\n                                        data={ingredient}\r\n                                        deleteIngredient={this.deleteIngredient}\r\n                                        editIngredient={this.openIngredient}\r\n                